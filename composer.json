{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "bluegreensheet/job-region-populator": "@dev",
    "craftcms/ckeditor": "4.11.0",
    "craftcms/cms": "5.8.22",
    "craftcms/feed-me": "6.11.0",
    "craftcms/stripe": "1.7.0",
    "doublesecretagency/craft-cpcss": "3.0.0",
    "nystudio107/craft-seomatic": "5.1.19",
    "ostark/craft-async-queue": "4.0.0",
    "putyourlightson/craft-dashboard-begone": "3.0.0",
    "servd/craft-asset-storage": "4.2.2",
    "studioespresso/craft-exporter": "5.1.1",
    "verbb/expanded-singles": "3.0.3",
    "verbb/formie": "3.1.8",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "./plugins/jobregionpopulator"
    },
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}
